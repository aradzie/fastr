{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig-template.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "build",
    "lib": ["dom", "dom.iterable"],
    "module": "commonjs",
    "target": "es2019",
    "incremental": true,
    "tsBuildInfoFile": "build/.tsbuildcache"
  },
  "include": [
    "src/**/*"
  ]
}
