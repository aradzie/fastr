{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig-template.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "lib",
    "lib": ["es2022", "dom", "dom.iterable"],
    "module": "esnext",
    "declaration": true,
    "incremental": true,
    "tsBuildInfoFile": "lib/.tsbuildcache"
  },
  "include": [
    "src/**/*"
  ]
}
